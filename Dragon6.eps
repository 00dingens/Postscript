/w3{30 cos}def   % 1/3 cos(30)
/w1{w3 3 div}def % 3/3 cos(30)
/w2{w1 2 mul}def % 2/3 cos(30)
/wh{w3 3 div}def % 1/3 cos(30)
/m1{w1 neg}def   %-1/3 cos(30)
/m2{w2 neg}def   %-2/3 cos(30)
/m3{w3 neg}def   %-3/3 cos(30)
/mh{wh neg}def   %-1/3 cos(30)
/l1{5 6 div}def  % 5/6
/l2{2 3 div}def  % 2/3
/l3{1 6 div}def  % 1/6
/lh{1 6 div}def  % 1/3
/j1{l1 neg}def   %-5/6
/j2{l2 neg}def   %-2/3
/j3{l3 neg}def   %-1/4
/jh{lh neg}def   %-1/3

%arrays mit den koordinaten der punkte
%/xr [w1 w2 w3 w3 w2 w1 m1 m2 m3 m3 m2 m1] def
%/yr [j1 j2 j3 l3 l2 l1 l1 l2 l3 j3 j2 j1] def
/xa [w1 w2 w3 w3 w2 w1 m1 m2 m3 m3 m2 m1] def
/ya [j1 j2 j3 l3 l2 l1 l1 l2 l3 j3 j2 j1] def
/xb [ 0 wh wh wh wh  0  0 mh mh mh mh  0] def
/yb [jh j3 j3 l3 l3 lh lh l3 l3 j3 j3 jh] def

/rand{1}def

/s{ %erwartet 2 nummern von 0 bis 7, verbindet die anschlüsse
xa 1 index get rand mul ya 2 index get rand mul moveto
xa 1 index get ya 2 index get lineto
xb 1 index get yb 2 index get
xb 4 index get yb 5 index get
xa 6 index get ya 7 index get curveto
xa 2 index get rand mul ya 3 index get rand mul lineto
stroke pop pop
}def

%/s{ %erwartet 2 nummern von 0 bis 7, verbindet die anschlüsse
%xa 1 index get rand mul ya 2 index get rand mul moveto
%xa 1 index get ya 2 index get lineto
%xb 1 index get yb 2 index get lineto
%xb 2 index get yb 3 index get lineto
%xa 2 index get ya 3 index get lineto %curveto
%xa 2 index get rand mul ya 3 index get rand mul lineto
%stroke pop pop
%}def

/r{1 0 0 setrgbcolor}def   %rot     -> punktsymmetrisch, 90°
/g{0 .7 0 setrgbcolor}def  %grün    -> achsensymmetrisch gerade
/y{.8 .7 0 setrgbcolor}def %gelb    -> achsensymmetrisch schräg
/b{0 0 1 setrgbcolor}def   %blau    -> punktsymmetrisch, 180°
/k{0 0 0 setrgbcolor}def   %schwarz -> nicht symmetrisch

%einkommentieren für 'alle schwarz'
%/r{k}def/g{k}def/b{k}def/y{k}def

