gsave
72 2.54 div dup scale	%72 dpi
10.8 13.97 translate %mitte

% hilfreiche Funktionen
/drehungen {dup 360 exch div exch} def /von {dup rotate} def
/sw {dup dup setrgbcolor} def
/gitter {
0.8 sw -30 .1 30 {-30 .1 30 {1 index 0.01 0 360 arc fill} for} for 
0.5 sw -30 .5 30 {-30 .5 30 {1 index 0.02 0 360 arc fill} for} for 
0.2 sw -30 1  30 {-30  1 30 {1 index 0.04 0 360 arc fill} for} for 
0   sw -30 5  30 {-30  5 30 {1 index 0.07 0 360 arc fill} for} for
} def
/hilfslinien4 {
0.01 setlinewidth 0.6 sw 128 drehungen {von 10 0 moveto 30 0 lineto stroke}repeat
0.01 setlinewidth 0.6 sw  64 drehungen {von  5 0 moveto 30 0 lineto stroke}repeat
0.01 setlinewidth 0.6 sw  32 drehungen {von  0 0 moveto 30 0 lineto stroke}repeat
0.01 setlinewidth 0.5 sw  16 drehungen {von  0 0 moveto 30 0 lineto stroke}repeat
0.01 setlinewidth 0.4 sw   8 drehungen {von  0 0 moveto 30 0 lineto stroke}repeat
} def
/hilfslinien6 {
0.01 setlinewidth 0.6 sw 96 drehungen {von 10 0 moveto 30 0 lineto stroke}repeat
0.01 setlinewidth 0.6 sw 48 drehungen {von  5 0 moveto 30 0 lineto stroke}repeat
0.01 setlinewidth 0.6 sw 24 drehungen {von  0 0 moveto 30 0 lineto stroke}repeat
0.01 setlinewidth 0.5 sw 12 drehungen {von  0 0 moveto 30 0 lineto stroke}repeat
0.01 setlinewidth 0.4 sw  6 drehungen {von  0 0 moveto 30 0 lineto stroke}repeat
} def
/hilfslinien7 {
0.01 setlinewidth 0.6 sw 112 drehungen {von 10 0 moveto 30 0 lineto stroke}repeat
0.01 setlinewidth 0.6 sw  56 drehungen {von  5 0 moveto 30 0 lineto stroke}repeat
0.01 setlinewidth 0.6 sw  28 drehungen {von  0 0 moveto 30 0 lineto stroke}repeat
0.01 setlinewidth 0.5 sw  14 drehungen {von  0 0 moveto 30 0 lineto stroke}repeat
0.01 setlinewidth 0.4 sw   7 drehungen {von  0 0 moveto 30 0 lineto stroke}repeat
} def
/kreise {
0.01  setlinewidth 0.7 sw 0 .5 30 {0 exch 0 exch 0 360 arc stroke} for
0.01  setlinewidth 0.5 sw 0 1  30 {0 exch 0 exch 0 360 arc stroke} for
%0.01  setlinewidth 0.4 sw 0 5  30 {0 exch 0 exch 0 360 arc stroke} for
} def

hilfslinien4
%hilfslinien6
%hilfslinien7
kreise
%gitter

showpage
grestore
