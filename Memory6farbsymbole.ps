/grid {
 0.02 setlinewidth
 0.5 dup dup setrgbcolor
 0 0 0 1 1 0 1 1 4 {0.02 0 360 arc fill} repeat
} def

/c{0 setcmykcolor}def
/r{rectfill}def

/draw{
0.1 setlinewidth
0 0 0 setrgbcolor
dup 0 eq {1 0 0 c 0.5 0.5 0.3 0 360 arc fill} if
dup 1 eq {0 1 0 c 0.5 0.5 0.3 0 360 arc fill} if
